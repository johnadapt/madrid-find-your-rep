{
    "name":"bright-cloud-studio/madrid-find-your-rep",
    "description":"A custom version of our Rep Locator package for Madrid",
    "keywords":["madrid", "find", "your", "rep", "bright", "cloud", "studio"],
    "type": "contao-bundle",
    "homepage": "http://brightcloudstudio.com/",
    "license": "LGPL-3.0+",
    "authors":[
        {
            "name": "Bright Cloud Studio",
            "homepage": "https://brightcloudstudio.com",
            "email": "john@brightcloudstudio.com",
            "role": "Developer"
        }
    ],
    "support":{
        "issues": "https://github.com/johnadapt/madrid-find-your-rep/issues",
        "source": "https://github.com/johnadapt/madrid-find-your-rep"
    },
    "require":{
        "php": "^7.3 || ^8.0",
        "contao/core-bundle": "^4.9 || ^5.3"
    },
    "extra":{
        "contao-manager-plugin": "Bcs\\MadridFindRepBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Bcs\\RecaptchaBundle\\": "src"
        },
        "classmap": [
            "src/Resources/contao"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    }
}